#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/bin/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/acorn@8.6.0/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/node_modules:/Users/nikaisokay/Developer/formaink/node_modules:/Users/nikaisokay/Developer/node_modules:/Users/nikaisokay/node_modules:/Users/node_modules:/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/espree@9.2.0/node_modules/espree/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/espree@9.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/bin/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/acorn@8.6.0/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/node_modules:/Users/nikaisokay/Developer/formaink/node_modules:/Users/nikaisokay/Developer/node_modules:/Users/nikaisokay/node_modules:/Users/node_modules:/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/espree@9.2.0/node_modules/espree/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/espree@9.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.6.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.6.0/node_modules/acorn/bin/acorn" "$@"
fi
