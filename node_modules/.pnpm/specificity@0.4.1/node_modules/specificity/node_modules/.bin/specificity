#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/bin/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/specificity@0.4.1/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/node_modules:/Users/nikaisokay/Developer/formaink/node_modules:/Users/nikaisokay/Developer/node_modules:/Users/nikaisokay/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/bin/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/specificity@0.4.1/node_modules/specificity/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/specificity@0.4.1/node_modules:/Users/nikaisokay/Developer/formaink/node_modules/.pnpm/node_modules:/Users/nikaisokay/Developer/formaink/node_modules:/Users/nikaisokay/Developer/node_modules:/Users/nikaisokay/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/specificity" "$@"
else
  exec node  "$basedir/../../bin/specificity" "$@"
fi
